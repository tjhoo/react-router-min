{
  "include": ["**/*", ".react-router/types/**/*"],
  "compilerOptions": {
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "noEmit": true,
    "paths": {
      "~/*": ["./app/*"]
    },
    "plugins": [
      {
        "name": "@react-router/dev"
      }
    ],
    "rootDirs": [".", "./.react-router/types"],
    "strict": true,
    "target": "ES2022",
    "types": ["@react-router/node", "vite/client"]
  }
}
